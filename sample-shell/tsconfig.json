{
  "compilerOptions": {
    "module": "esnext",
    "jsx": "react",
    "outDir": "./lib",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "es6",
    "sourceMap": true,
    "noEmit": true,
    "importHelpers": true,
    "lib": ["dom", "dom.iterable", "esnext"]
  },
  "include": ["./src"]
}
